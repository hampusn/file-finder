  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hampusn/file-finder/issues"
  },
  "homepage": "https://github.com/hampusn/file-finder#readme",
  "devDependencies": {
    "inquirer": "^5.2.0",
    "zip-dir": "^1.0.2",
    "jsonfile": "^4.0.0",
    "request": "^2.85.0"
  }
}

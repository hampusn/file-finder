{
  "name": "file-finder",
  "version": "0.1.0",
  "description": "Find files in the file archive and files uploaded on pages.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "setup": "node util/setup.js",
    "zip": "node util/zip.js",
    "deploy": "npm run zip && node util/deploy.js",
